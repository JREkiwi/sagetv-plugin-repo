		<Plugin>json</Plugin>

	</Dependency>

	<Dependency>

		<Plugin>gson</Plugin>

	</Dependency>

       <Package>

               <PackageType>JAR</PackageType>

               <Location>http://bitbucket.org/skiingwiz/sagetv-sre/downloads/livepvrdata4j-5.0.0.1.zip</Location>

               <MD5>aebcc1d091bcd84e6d3adf3c94fce1a8</MD5>

